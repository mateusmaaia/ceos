<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="leviata.ceos">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_no_background"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".login.register.RegisterActivity"/>
        <activity android:name=".MainActivity"/>
        <activity android:name=".login.register.ForgotPasswordActivity"/>
        <activity android:name=".HomeActivity"/>
        <activity android:name=".MenuFragment"/>
        <activity android:name=".ProfileFragment"/>
        <activity android:name=".navigation.NavigationActivity"/>
        <activity android:name=".login.register.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>